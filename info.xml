<?xml version="1.0"?>
<extension key="lalg-civi-sync" type="module">
  <file>lalg_civi_sync</file>
  <name>LALG Civi Sync</name>
  <description>Synchronise Drupal User details etc. with CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>maynardsmith</author>
    <email>tony.maynard-smith@ntlworld.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-07-24</releaseDate>
  <version>1.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>This is first version of a new module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/LalgCiviSync</namespace>
    <angularModule>crmLalgCiviSync</angularModule>
    <format>23.02.1</format>
  </civix>
  <upgrader>CRM_LalgCiviSync_Upgrader</upgrader>
  <mixins>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
</extension>
